.SwipeImg_box{
  width: 100%;
  height: 800rpx;
}
.SwipeImg_box_swiper{
  width: 100%;
  height: 100%;
}
.SwipeImg_box_img{
  width: 100%;
  height: 100%;
}
.Commodity_information{
  width: 100%;
  height: 130rpx;
  padding: 30rpx 10rpx 0 16rpx;
  box-sizing: border-box;
}
.Free_shipping{
  border: 1px solid #ccc;
  border-radius: 10rpx;
  font-size: 26rpx;
  margin:0rpx 20rpx 0rpx 30rpx;
}
.commodity_title{
  font-size: 28rpx;
  font-weight: 600;
}
.commodity_Price{
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
}
.money{
  font-size: 40rpx;
  font-weight: 600;
  color: red;
  float: left;
  margin-left: 30rpx;
}
.Chinamoney{
  font-size: 30rpx;
}
.Salesvolume{
  font-size: 26rpx;
  color:#c2b1c5;
  float: right;
  margin-right: 20rpx;
}
.imgtext{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
}
.imgtext .line{
  width: 100rpx;
  height: 1px;
  background: #ccc;
  display: inline-block;
  margin: 0 30rpx 10rpx 30rpx ;
}
.imgtext .text{
  font-size: 26rpx;
}
.list_img{
  width:100%;
  height: 1000rpx;
}
.list_img image{
  width: 100%;
  height: 100%;
}
.totaobao{
  width: 100%;
  height: 120rpx;
  background: red;
  position: fixed;
  bottom: 0;
  font-size: 36rpx;
  font-weight: 600;
  color: #fff;
}