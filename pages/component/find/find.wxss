.vansearch{
  position: fixed;
  top: 0;
  width: 100%;
}
.fine_List_box{
  width: 100%;
  height: 80rpx;
  padding-left: 25rpx;
  position: fixed;
  top: 99rpx;
  background: #fff;
  box-sizing: border-box;
}
.fine_scrollview{
  width: 90%;
  white-space: nowrap;
  height: 100%;
  float: left;
}
.fortext{
  display: inline-block;
  height: 100%;
  width: 15%;
}
.fortext .text{
  height: 60rpx;
  line-height: 60rpx;
}
.fine_List_right{
  float: right;
  width: 10%;
  height: 60rpx;
  text-align: center;
  line-height: 68rpx;
  font-size: 40rpx;
  border-left: 1px solid #ccc;
  box-sizing: border-box;
}
.active{
  font-size: 40rpx;
  font-weight: 600;
  color: red;
}